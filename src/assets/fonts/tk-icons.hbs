/**************************** Icon Font *****************************/
@font-face {
    font-family: "{{ fontName }}";
  src: url("../src/assets/fonts/tk-icons.eot?4cebe2ff0a7d886f0eb917b08c763f05?#iefix") format("embedded-opentype"),
url("../src/assets/fonts/tk-icons.woff2?4cebe2ff0a7d886f0eb917b08c763f05") format("woff2"),
url("../src/assets/fonts/tk-icons.woff?4cebe2ff0a7d886f0eb917b08c763f05") format("woff"),
url("../src/assets/fonts/tk-icons.ttf?4cebe2ff0a7d886f0eb917b08c763f05") format("truetype"),
url("../src/assets/fonts/tk-icons.svg?4cebe2ff0a7d886f0eb917b08c763f05#tk-icons") format("svg");
  }

  i[class^="tk-icon-"]:before, i[class*=" tk-icon-"]:before {
    font-family: {{fontName}} !important;
    font-style: normal;
    font-weight: normal !important;
    vertical-align: top;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

{{# each codepoints }}
.{{ ../classPrefix }}{{ @key }}:before {
    content: "\\{{ this }}";
}
{{/ each }}