{
  "name": "@symphony/uitoolkit-styles",
  "version": "1.0.2",
  "description": "Symphony UI design system",
  "publishConfig": {
    "registry": "https://repo.symphony.com/artifactory/api/npm/npm-local-dev2"
  },
  "main": "index.css",
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "sass --load-path=node_modules --style compressed uitoolkit.scss:dist/css/uitoolkit.css uitoolkit-tempo-support.scss:dist/css/uitoolkit-tempo-support.css",
    "build-storybook": "build-storybook -c .storybook -o dist/storybook",
    "sass-watch": "sass --watch --load-path=node_modules --style compressed uitoolkit.scss:dist/css/uitoolkit.css uitoolkit-tempo-support.scss:dist/css/uitoolkit-tempo-support.css"
  },
  "keywords": [
    "scss",
    "sass",
    "css",
    "design"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-storysource": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/html": "5.3.19",
    "babel-loader": "^8.1.0",
    "sass": "^1.22.10",
    "sass-loader": "^7.2.0",
    "storybook-dark-mode": "^0.4.1",
    "@symphony/tempo-ui": "^0.0.54"
  },
  "dependencies": {}
}
