{
  "name": "@symphony/uitoolkit-styles",
  "version": "1.1.0",
  "description": "Symphony UI design system",
  "publishConfig": {
    "registry": "https://repo.symphony.com/artifactory/api/npm/npm-local-dev2"
  },
  "main": "index.css",
  "scripts": {
    "prestart": "yarn icons",
    "start": "start-storybook -p 6006 -c .storybook watch-css -s ./src",
    "prebuild": "yarn icons && sh src/scripts/generate-cdn.sh",
    "build-storybook": "build-storybook -c .storybook -o dist/storybook",
    "build": "yarn sass --load-path=node_modules --no-source-map --style compressed uitoolkit.scss:dist/css/uitoolkit.css uitoolkit-tempo-support.scss:dist/css/uitoolkit-tempo-support.css uitoolkit-cdn.generated.scss:dist/css/uitoolkit-cdn.generated.css",
    "watch": "sass --watch --no-source-map --load-path=node_modules --style compressed uitoolkit.scss:dist/css/uitoolkit.css",
    "icons": "node src/scripts/generate-icons"
  },
  "keywords": [
    "scss",
    "sass",
    "css",
    "design"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-storysource": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/html": "5.3.19",
    "@symphony/tempo-ui": "^0.0.54",
    "babel-loader": "^8.1.0",
    "icon-font-generator": "^2.1.10",
    "markdown-loader": "^6.0.0",
    "sass": "^1.22.10",
    "sass-loader": "^7.2.0",
    "storybook-dark-mode": "^0.4.1"
  }
}
